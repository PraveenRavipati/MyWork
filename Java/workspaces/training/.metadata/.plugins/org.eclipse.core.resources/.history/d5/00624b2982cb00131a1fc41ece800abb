package com.theard.basics;

public class LifeCycle {
	Iclass i=new Iclass();
	public static void main(String[] args) {
		int n=10;
		
		MyThread m[]=new MyThread[n];
		for(int j=0;j<n;j++)
		{
			m[j]=new MyThread();
			m[j].start();
		}
	}
	public Iclass getIclass()
	{
		return i;
	}

}
class MyThread extends Thread
{
	
	public void run() {
		for(int k=0;k<100;k++)
		{
			
		}
	}
}
class Iclass
{
	private int i=0;
	public void incrI() {
		i++;
	}
	public void decI()
	{
		i--;
	}
	public int getI() {
		return i;
	}
	
}
