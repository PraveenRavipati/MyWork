package com.binarysearch.rules;

import com.binarysearch.bean.Employee;

public class MyCriteria {
	int checkCodition(Object Element,Object searchKey)
	{
		int status=1;
		Employee emp
		Employee key=(Employee) searchKey;
		if((key.getEmpId())!=0)
		{
			if(emp.getEmpId()==key.getEmpId())
			{
				status=0;
				return status;
			}
			else if (emp.getEmpId()>key.getEmpId()) {
				status=1;
				return status;
			}
			else {
				status=-1;
				return status;
			}
		}else if((emp.getEmpName())!=null)
		{
			status=searchName.compareToIgnoreCase(EmployeeName);
			return status;
		}
		else
		{
			System.out.println("sorry try agin");
		}
		return 1;
	}
}
