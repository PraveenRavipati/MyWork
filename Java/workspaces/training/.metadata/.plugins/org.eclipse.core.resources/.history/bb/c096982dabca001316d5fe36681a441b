package com.theard.basics;

public class Lifecycle {

	public static void main(String[] args) {
		
	}

}
class MyTheard extends Thread
{
	private int i=0;
	private void incrI() {
		i++;
	}
	private int getI() {
		return i;
	}
	public void run() {
		for(int j=0;j<10;j++)
		{
			incrI();
			System.out.println("When j:"+j);
		}
		
	}
}