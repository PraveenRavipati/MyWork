package bankTransactionSampleProject.testcases;

public class Account{  
    
    synchronized void showAccount(String accountName){  
        System.out.println("My account name is "+accountName+" Holder Name is "+Thread.currentThread().getName());  
        try{  
          Thread.sleep(500);  
        }catch(Exception e){}  
      }  
   }  
     
   class MyThread1 extends Thread{  
       public void run(){
    	   Account1 acc=new Account1();
          acc.showAccount("Dineshonjava.com");  
     }  
   }  
    
   class MyThread2 extends Thread{  
       public void run(){  
    	  Account1 acc=new Account1();
          acc.showAccount("Linkedin.com");  
       }  
   }  
     
   class MyThread3 extends Thread{  
       public void run(){  
    	   Account1 acc=new Account1();
          acc.showAccount("Facebook.com");  
       }  
   }  
     
   class MyThread4 extends Thread{  
       public void run(){  
    	   Account1 acc=new Account1();
          acc.showAccount("Twitter.com");  
       }  
   }  
     
   class StaticSyncDemo{  
       public static void main(String t[]){  
          MyThread1 t1 = new MyThread1();
          MyThread1 t5 = new MyThread1();  
          MyThread2 t2 = new MyThread2();  
          MyThread3 t3 = new MyThread3();  
          MyThread4 t4 = new MyThread4();  
          t1.setName("DAV JavaServices");
          t5.setName("PRAVEEN JavaServices");  
          t2.setName("dinesh.rajput");  
          t3.setName("dineshonjava");  
          t4.setName("admin@dineshonjava.com");         
          t1.start();  
          t5.start(); 
          t2.start();  
          t3.start();  
          t4.start();  
       }  
   }  
